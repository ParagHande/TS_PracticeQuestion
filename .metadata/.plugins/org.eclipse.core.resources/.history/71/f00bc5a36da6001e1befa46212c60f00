package com.practice.ts2;

import java.util.Arrays;

public class ReverseArray {
	
	public static void main(String[] args) {
		int[] arrayNumber = {1,2,3,4,5,6,7,8,9};
		System.out.println("Given Array : "+Arrays.toString(arrayNumber));
		System.out.println(arrayNumber.length);
		for(int i=arrayNumber.length-1; i<=arrayNumber.length+2; i--) {
			System.out.println(arrayNumber[i]);
		}
		
		
		
	}

	

}
